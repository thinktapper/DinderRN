{
  "cli": {
    "version": ">= 3.5.2"
  },
  "build": {
    "development": {
      "env": {
        "APP_VARIANT": "development"
      },
      "developmentClient": true,
      "distribution": "internal",
      "channel": "local",
      "ios": {
        "simulator": true,
        "resourceClass": "m1-medium"
      }
    },
    "development-simulator": {
      "developmentClient": true,
      "distribution": "internal",
      "ios": {
        "simulator": true,
        "resourceClass": "m1-medium"
      },
      "channel": "development-simulator"
    },
    "preview": {
      "distribution": "internal",
      "ios": {
        "resourceClass": "m1-medium",
        "simulator": true
      },
      "channel": "staging"
    },
    "production": {
      "ios": {
        "resourceClass": "m1-medium"
      },
      "channel": "production"
    }
  },
  "submit": {
    "production": {}
  }
}
